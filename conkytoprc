background no
out_to_console
update_interval 1
use_spacer none

TEXT
^fg(\#92b19e)CPU^fg():^fg(\#92b19e)${cpu}%${if_gw}${if_existing /proc/net/route wlan0} ^fg():: wlan0: ^fg(\#99cc33)^i(.dzen/down.xbm) ${downspeedf wlan0} kB/s ^fg(\#ff3300)^i(.dzen/up.xbm) ${upspeedf wlan0} kB/s${endif}${if_existing /proc/net/route eth0} ^fg():: eth0: ^fg(\#99cc33)^i(.dzen/down.xbm) ${downspeedf eth0} kB/s ^fg(\#ff3300)^i(.dzen/up.xbm) ${upspeedf eth0} kB/s${endif}${endif} ^fg():: ^fg(\#00ff00)${if_match ${battery_percent BAT1} < 50}^fg(\#ffff00)${endif}${if_match ${battery_percent BAT1} < 10}^fg(\#ff0000)${endif}${battery_percent BAT1} ^fg():: ^fg(\#ffffff)${time %a %b %d %H:%M:%S} 

